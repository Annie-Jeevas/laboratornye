<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>LoginBean</managed-bean-name>
        <managed-bean-class>Beans.LoginBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mRequestBean</managed-bean-name>
        <managed-bean-class>Beans.MRequestBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sessionBean</managed-bean-name>
        <managed-bean-class>Beans.SessionBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>SFSB</managed-bean-name>
        <managed-bean-class>Beans.SFSB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
        <from-view-id>/real_home.xhtml</from-view-id>       
        <navigation-case>
            <from-outcome>case1</from-outcome>            
            <to-view-id>/book_description.xhtml</to-view-id>
        </navigation-case>        
        <navigation-case>
            <from-outcome>grade</from-outcome>
            <to-view-id>/BookGrade.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/book_description.xhtml</from-view-id>
        <redirect>
            <view-param>
                <name>param</name>
                <value>bar</value>
            </view-param>
        </redirect>
        <navigation-case>
            <from-outcome>fail</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addingToMyBooksOk</from-outcome>
            <to-view-id>/user_profile.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/createComment.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/real_home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/user_profile.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>case1</from-outcome>
            <to-view-id>/book_description.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>EditReq</from-outcome>
            <to-view-id>/editReq.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editComm</from-outcome>
            <to-view-id>/editComm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/editReq.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editReqOk</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/user_profile.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editReqFail</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/editReq.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/editComm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editCommOk</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/user_profile.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editCommFail</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/editComm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/register.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addingVisitorOk</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/user_profile.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addingVisitorFail</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/register.xhtml</to-view-id>
        </navigation-case>        
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/createReq.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>okCreateReq</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/my_requests.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failCreateReq</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/createReq.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/addBook.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>okAddingBook</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/real_home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failAddingBook</from-outcome>
            <redirect>
                <view-param>
                    <name>param</name>
                    <value>bar</value>
                </view-param>
            </redirect>
            <to-view-id>/addBook.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
